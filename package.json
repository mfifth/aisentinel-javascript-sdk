{
  "name": "@aisentinel/javascript-sdk",
  "version": "0.1.0",
  "description": "Isomorphic JavaScript SDK for AISentinel governance platform.",
  "type": "module",
  "main": "./node/dist/index.cjs",
  "module": "./node/dist/index.js",
  "types": "./node/dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./node/dist/index.d.ts",
      "browser": {
        "import": "./browser/dist/index.js",
        "require": "./browser/dist/index.cjs"
      },
      "node": {
        "import": "./node/dist/index.js",
        "require": "./node/dist/index.cjs"
      },
      "default": "./node/dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf browser/dist node/dist",
    "build": "npm run clean && rollup -c",
    "build:browser": "rollup -c --environment BUILD:browser",
    "build:node": "rollup -c --environment BUILD:node",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json",
    "lint": "eslint 'src/**/*.ts'",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "prepare": "npm run build"
  },
  "keywords": [
    "aisentinel",
    "governance",
    "compliance",
    "sdk",
    "isomorphic"
  ],
  "author": "AISentinel",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "browser/dist",
    "node/dist",
    "src",
    "examples",
    "tsconfig.json",
    "rollup.config.mjs",
    "README.md"
  ],
  "browser": {
    "fs": false,
    "path": false
  },
  "dependencies": {
    "cross-fetch": "^3.1.8",
    "idb-keyval": "^6.2.1",
    "node-fetch": "^3.3.2",
    "pako": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.19.21",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.18.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  }
}
